{
  "include": [
    "src/*.tsx"
  ],
  "exclude": [],
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "noEmitOnError": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "commonjs",
    "target": "ES2017",
    "lib": [
      "DOM",
      "ES2017"
    ]
  },
  "types": [
    "emscripten"
  ]
}